    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <!--MQTT地址-->
  <connectionStrings>
    <add name="ServerUrl" connectionString="10.13.18.40"/>
  </connectionStrings>
  <appSettings>
    <!--MQTT连接参数-->
    <add key ="Port" value ="1883"/>
    <add key ="UserId" value ="admin"/>
